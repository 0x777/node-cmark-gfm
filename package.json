{
  "name": "cmark-gfm",
  "version": "0.2.0",
  "description": "Node bindings to GitHub's GFM-enhanced fork of the cmark Markdown parser",
  "repository": "https://github.com/BinaryMuse/node-cmark-gfm",
  "main": "index.js",
  "scripts": {
    "build": "node-gyp rebuild",
    "test": "mocha test/helper.js test/*.test.js"
  },
  "keywords": [
    "markdown",
    "cmark",
    "commonmark",
    "gfm",
    "cmark-gfm"
  ],
  "author": "Michelle Tilley <michelle@michelletilley.net>",
  "license": "MIT",
  "files": [
    "binding.gyp",
    "index.js",
    "README.md",
    "src/**/*",
    "cmark/CMakeLists.txt",
    "cmark/COPYING",
    "cmark/FindAsan.cmake",
    "cmark/Makefile",
    "cmark/Makefile.nmake",
    "cmark/nmake.bat",
    "cmark/README.md",
    "cmark/suppressions",
    "cmark/toolchain-mingw32.cmake",
    "cmark/src/**/*",
    "cmark/extensions/**/*",
    "cmark/api_test/**/*",
    "cmark/man/**/*",
    "cmark/test/**/*",
    "test/**/*"
  ],
  "dependencies": {
    "bindings": "^1.4.0",
    "cmake-js": "^5.0.1",
    "node-addon-api": "*"
  },
  "devDependencies": {
    "@alrra/travis-scripts": "^3.0.1",
    "chai": "^3.5.0",
    "chai-html": "^1.1.0",
    "dedent": "^0.6.0",
    "mocha": "^5.2.0",
    "node-gyp": "^3.8.0"
  }
}
